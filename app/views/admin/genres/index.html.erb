<h1>ジャンル一覧・追加</h1>
<%= render 'form', genre: @genre %>
<table class=‘table table-hover table-inverse’>
  <thead>
    <tr>
      <th>ジャンル</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% @genres.each do |genre| %>
      <tr>
        <td><%= genre.name %></td>
        <td>
          <%= link_to edit_admin_genre_path(genre) do %>
          編集する <!--ここボタンにしてください-->
          <% end %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
