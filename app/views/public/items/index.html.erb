<div class="container">
  <div class="row">
    <div class="col-md-12">
      <p><strong>商品一覧</strong>(全<%= @items.count %>件)</p>
      <% @items.each do |item| %>
        <%= attachment_image_tag(item, :image, :fill, 50, 100) %>
        <%= item.name %>
        <%= item.price %>
      <% end %>
      <%= paginate @items %>
   </div>
  </div>
</div>